<!DOCTYPE html>
<html>
<head>
  <title>Kindervibe</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
<div id="wrapper">
  <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
      </button>
    </div>
    <ul class="nav navbar-top-links navbar-right navbar-collapse">
      <li><%= link_to 'Days', app_days_path %></li>
      <li><%= link_to 'Childern', childerns_path %></li>
      <li><%= link_to 'Food', food_activities_path%></li>
      <li><%= link_to 'Photos', images_path%></li>
      <li><%= link_to 'Posts', posts_path%></li>
    </ul>
  </nav>
  <div id="page-wrapper" style="margin-left: 10px;">
    <%= yield %>
  </div>
</div>
</body>
</html>
